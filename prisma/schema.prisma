generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tbl_all_surname {
  id                  Int       @id @default(autoincrement())
  surname_id          String    @db.VarChar(250)
  assembly_no         Int
  count_num           Int
  surname             String    @db.VarChar(250)
  surname_en          String    @db.VarChar(250)
  surname_uni         String    @db.VarChar(100)
  surname_flag        Int       @default(0)
  surname_similar     String    @db.VarChar(250)
  surname_caste       String    @db.VarChar(250)
  surname_caste_en    String    @db.VarChar(250)
  caste_flag          Int       @default(0)
  surname_category    String    @db.VarChar(250)
  surname_category_en String    @db.VarChar(250)
  category_flag       Int       @default(0)
  updation_date       DateTime? @db.Date
}

model assembly_paper {
  id            Int    @id @default(autoincrement())
  assembly_id   Int    @unique
  assembly_name String
}


